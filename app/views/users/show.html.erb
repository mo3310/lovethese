<main>
<div class="container">
  <div style="color: green;"><%= flash[:notice] %></div>
  <div class="row no-gutters">
    <div class="col-md-3">
      <%= render 'users/info', user: @user, post: @post %>
    </div>
    <div class="col-md-8 offset-md-1">
      <%= render 'posts/list', posts: @posts %>
    </div>
  </div>
</div>
</main>